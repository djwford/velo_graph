<%= simple_form_for @workoutSession do |f|%>
  <%= f.input :workout_date %>
  <%= f.input :name %>
  <%= f.input :workout %>
  <%= f.input :notes, as: :text %>
  <%= f.button :submit, :label => "Finish Workout" %>
<% end %>

<div id="start_button" data-active="false"><p>Start</p></div>
<div id="pause_button"><p>Pause</p></div>

<div id="speed_graph" style="width:100%; height:400px;"></div>
